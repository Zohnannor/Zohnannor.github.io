{
    "name": "float-calculator",
    "version": "1.0.0",
    "private": true,
    "homepage": "http://zohnannor.github.io/float-calculator",
    "dependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.5.0",
        "@testing-library/user-event": "^7.2.1",
        "gh-pages": "^3.1.0",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-scripts": "3.4.3"
    },
    "scripts": {
        "start": "set PORT=25565 &&react-scripts start",
        "build": "react-scripts build",
        "eject": "react-scripts eject",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
